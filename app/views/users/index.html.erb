<div class="container">
    <%= form_for :user, url: userEntry_url do |f| %>
        <p>
            <%= f.label :name %><br>
            <%= f.text_field(:name, {:class => 'form-control'}) %>
        </p>
        <p>
            <%= f.submit({:class => 'btn btn-primary'}) %>
        </p>
    <% end %>
</div>
